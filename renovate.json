{
  "extends": [
    ":separateMajorReleases",
    ":combinePatchMinorReleases",
    ":ignoreUnstable",
    ":prImmediately",
    ":semanticPrefixFixDepsChoreOthers",
    ":updateNotScheduled",
    ":ignoreModulesAndTests",
    ":autodetectPinVersions",
    ":prConcurrentLimit20",
    "workarounds:all"
  ],
  "addLabels": ["renovatebot"],
  "major": {},
  "minor": {
    "packageRules": [
      {
        "packagePatterns": [".*"],
        "labels": [
          "dependencies",
          "automerge"
        ]
      }
    ]
  },
  "patch": {
    "packageRules": [
      {
        "packagePatterns": [".*"],
        "labels": [
          "dependencies",
          "automerge"
        ],
        "automerge": true
      }
    ]
  }
}
